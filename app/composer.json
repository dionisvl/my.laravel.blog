{
    "name": "dionisvl/my.laravel.blog",
    "type": "project",
    "description": "Laravel ecommerce/blog template engine",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "cboden/ratchet": "^0.4.3",
        "darkaonline/l5-swagger": "^8.0",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "itsgoingd/clockwork": "^5.0",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^8.0",
        "laravel/helpers": "^1.2",
        "laravel/telescope": "^4.0",
        "laravel/tinker": "^2.0",
        "laravelcollective/html": "^6.0",
        "laravelium/sitemap": "^8.0",
        "maatwebsite/excel": "^3.1",
        "vearutop/php-obscene-censor-rus": "^1.0",
        "wulfheart/pretty_routes": "^0.3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.3",
        "barryvdh/laravel-ide-helper": "^2.8",
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.0.0",
        "codeception/module-db": "^1.0",
        "codeception/module-laravel5": "^1.1",
        "codeception/module-phpbrowser": "^1.0.0",
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpstan/phpstan": "^1.4",
        "phpstan/phpstan-beberlei-assert": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpstan/phpstan-strict-rules": "^1.1",
        "phpunit/phpunit": "^9.0",
        "roave/security-advisories": "dev-latest"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Dionisvl\\Shop\\": "packages/shop/src/",
            "Dionisvl\\FrontParts\\": "packages/frontparts/src/",
            "Dionisvl\\Chat\\": "packages/chat/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "phpcs": "phpcs --standard=PSR12",
        "phpstan": "phpstan analyse --ansi --memory-limit 1G"
    }
}
